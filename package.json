{
  "name": "portfolio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "build-prisma": "bunx prisma generate",
    "prisma-push": "cd prisma && bunx prisma db push",
    "prisma-all": "bun run build-prisma && bun run prisma-push"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "autoprefixer": "^10.4.20",
    "flowbite": "^2.5.1",
    "flowbite-svelte": "^0.46.15",
    "flowbite-svelte-icons": "^1.6.1",
    "postcss": "^8.4.42",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "5.19.0",
    "prisma": "^5.19.0",
    "sqlite": "^5.1.1",
    "svelte-scrolling": "^1.4.0"
  }
}
